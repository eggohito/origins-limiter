{
    "type": "origins:action_over_time",
    "interval": 1,
    "rising_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "function #origins-limiter:cancel/pre"
            },
            {
                "type": "origins:execute_command",
                "command": "tellraw @s {\"nbt\": \"root.msg.cancel.\\\"origins:origin\\\"\", \"storage\": \"origins-limiter:main\", \"interpret\": true}"
            },
            {
                "type": "origins:give",
                "stack": {
                    "item": "origins:orb_of_origin"
                }
            },
            {
                "type": "origins:execute_command",
                "command": "origin set @s origins:origin origins-limiter:empty"
            },
            {
                "type": "origins:execute_command",
                "command": "origin set @s origins-limiter:origins/origin/confirm origins-limiter:empty"
            },
            {
                "type": "origins:execute_command",
                "command": "function #origins-limiter:cancel/post"
            }
        ]
    },
    "hidden": true
}