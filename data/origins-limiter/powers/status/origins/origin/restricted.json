{
    "type": "origins:action_over_time",
    "interval": 1,
    "rising_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "origin set @s origins:origin origins-limiter:empty"
            },
            {
                "type": "origins:execute_command",
                "command": "origin set @s origins-limiter:confirm/origins/origin origins-limiter:pass"
            },
            {
                "type": "origins:execute_command",
                "command": "tellraw @s {\"storage\": \"origins-limiter:main\", \"nbt\": \"root.msg.cancel.\\\"origins:origin\\\"\", \"interpret\": true}"
            },
            {
                "type": "origins:execute_command",
                "command": "advancement grant @s only origins-limiter:status/restrict"
            }
        ]
    },
    "hidden": true
}